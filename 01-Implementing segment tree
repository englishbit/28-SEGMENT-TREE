#include<iostream>
using namespace std;
#define max 100000
int array[max]={0};

int main(){
    int n;
    cin>>n;
    for(int i=0;i<n;i++){
        cin>>array[i];
    }

    int segmentree[max*4]={0};

    for(int i=0;i<n;i++){
        segmentree[n+i]=array[i];
    }

    for(int i=n-1;i>0;i--){
        segmentree[i]=segmentree[i*2]+segmentree[(i*2)+1];
    }

    for(int i=1;i<2*n;i++){
        cout<<segmentree[i];
    }
    cout<<endl;

    return 0;
}